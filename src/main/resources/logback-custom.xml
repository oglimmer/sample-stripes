<configuration debug='true'>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %level [%thread] %logger{10} [%file:%line] %msg%n</pattern>
        </encoder>
    </appender>
    
    <root>
        <level value="debug"/>
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>
